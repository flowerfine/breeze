<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.sliew.scaleph.dao.mapper.master.ws.WsFlinkKubernetesJobMapper">
    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="cn.sliew.scaleph.dao.entity.master.ws.WsFlinkKubernetesJob">
        <result column="id" property="id"/>
        <result column="creator" property="creator"/>
        <result column="create_time" property="createTime"/>
        <result column="editor" property="editor"/>
        <result column="update_time" property="updateTime"/>
        <result column="name" property="name"/>
        <result column="job_id" property="jobId"/>
        <result column="execution_mode" property="executionMode"/>
        <result column="flink_deployment_mode" property="flinkDeploymentMode"/>
        <result column="flink_deployment_id" property="flinkDeploymentId"/>
        <result column="flink_session_cluster_id" property="flinkSessionClusterId"/>
        <result column="type" property="type"/>
        <result column="flink_artifact_jar_id" property="flinkArtifactJarId"/>
        <result column="flink_artifact_sql_id" property="flinkArtifactSqlId"/>
        <result column="remark" property="remark"/>
        <association column="flink_deployment_id"
                     property="flinkDeployment"
                     javaType="cn.sliew.scaleph.dao.entity.master.ws.WsFlinkKubernetesDeployment"
                     select="cn.sliew.scaleph.dao.mapper.master.ws.WsFlinkKubernetesDeploymentMapper.selectById"
                     resultMap="cn.sliew.scaleph.dao.mapper.master.ws.WsFlinkKubernetesDeploymentMapper.BaseResultMap"/>
        <association column="flink_deployment_id"
                     property="flinkSessionCluster"
                     javaType="cn.sliew.scaleph.dao.entity.master.ws.WsFlinkKubernetesSessionCluster"
                     select="cn.sliew.scaleph.dao.mapper.master.ws.WsFlinkKubernetesSessionClusterMapper.selectById"
                     resultMap="cn.sliew.scaleph.dao.mapper.master.ws.WsFlinkKubernetesSessionClusterMapper.BaseResultMap"/>

        <association column="flink_artifact_jar_id"
                     property="flinkArtifactJar"
                     javaType="cn.sliew.scaleph.dao.entity.master.ws.WsFlinkArtifactJar"
                     select="cn.sliew.scaleph.dao.mapper.master.ws.WsFlinkArtifactJarMapper.selectOne"
                     resultMap="cn.sliew.scaleph.dao.mapper.master.ws.WsFlinkArtifactJarMapper.FlinkArtifactJarMap"/>
        <association column="flink_artifact_jar_id"
                     property="flinkArtifactJar"
                     javaType="cn.sliew.scaleph.dao.entity.master.ws.WsFlinkArtifactJar"
                     select="cn.sliew.scaleph.dao.mapper.master.ws.WsFlinkArtifactSqlMapper.selectOne"
                     resultMap="cn.sliew.scaleph.dao.mapper.master.ws.WsFlinkArtifactSqlMapper.FlinkArtifactSqlMap"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, creator, create_time, editor, update_time,
        `name`, job_id, execution_mode,
        flink_deployment_mode, flink_deployment_id, flink_session_cluster_id
        `type`, flink_artifact_jar_id, flink_artifact_jar_id, remark
    </sql>

    <select id="list" resultMap="BaseResultMap">
        SELECT
        <include refid="Base_Column_List"/>
        FROM ws_flink_kubernetes_job
        WHERE
        <if test="executionMode != null">
            AND t1.execution_mode = #{executionMode}
        </if>
        <if test="flinkJobType != null">
            AND t1.type = #{flinkJobType}
        </if>
        <if test="flinkDeploymentMode != null">
            AND t1.flink_deployment_mode = #{flinkDeploymentMode}
        </if>
        <if test="name != null and name != ''">
            AND t1.name like concat('%', #{name}, '%')
        </if>
    </select>
</mapper>
